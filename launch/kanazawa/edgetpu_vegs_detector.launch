<launch>
  <arg name="INPUT_IMAGE"/>
  
  <!-- add my models -->
  <!-- <arg name="model_file" default="$(find grasp_recognition)/models/20200606-203100-vegs_voc_dataset/output_tflite_graph_edgetpu.tflite"/> -->
  <!-- <arg name="model_file" default="$(find grasp_recognition)/models/20200606-211753-vegs_small_datasets/output_tflite_graph_edgetpu.tflite"/> -->
  <!-- <arg name="model_file" default="$(find grasp_recognition)/models/20200606-214000-vegs_small_datasets/output_tflite_graph_edgetpu.tflite"/> -->
  <arg name="model_file" default="$(find grasp_recognition)/models/20200607-013746-vegs_v3_datasets/output_tflite_graph_edgetpu.tflite"/>
  
  <arg name="label_file" default="$(find grasp_recognition)/models/20200607-013746-vegs_v3_datasets/labels.txt"/>

  <node name="edgetpu_object_detector"
        pkg="coral_usb" type="edgetpu_object_detector.py"
        output="screen" respawn="true">
    <remap from="~input" to="$(arg INPUT_IMAGE)" />
    <rosparam subst_value="true" >
      model_file: $(arg model_file)
      label_file: $(arg label_file)
    </rosparam>
  </node>
</launch>
